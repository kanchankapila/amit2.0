{
  "name": "stockinsights",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/stockinsights/stats.json",
    "analyze:sourcemap": "source-map-explorer dist/stockinsights/**/*.js",
    "docs": "compodoc -p tsconfig.json -s",
    "docs:build": "compodoc -p tsconfig.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:ssr:stockinsights": "node dist/stockinsights/server/server.mjs",
    "dev:ssr": "ng run stockinsights:serve-ssr",
    "serve:ssr": "node dist/stockinsights/server/main.js",
    "build:ssr": "ng build && ng run stockinsights:server",
    "prerender": "ng run stockinsights:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/cdk": "^17.2.0",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/material": "^17.2.0",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/platform-server": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@angular/service-worker": "^17.2.0",
    "@angular/ssr": "^17.2.0",
    "@netlify/functions": "^2.6.0",
    "@popperjs/core": "^2.11.8",
    "@supabase/supabase-js": "^2.39.0",
    "apexcharts": "^3.45.0",
    "bootstrap": "^5.3.2",
    "chart.js": "^4.4.1",
    "echarts": "^5.4.3",
    "express": "^4.18.2",
    "ng-apexcharts": "1.7.6",
    "ng2-charts": "^5.0.4",
    "ngx-echarts": "^17.1.0",
    "primeicons": "^6.0.1",
    "primeng": "^17.7.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.0",
    "@angular/cli": "^17.2.0",
    "@angular/compiler-cli": "^17.2.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.3.2",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
